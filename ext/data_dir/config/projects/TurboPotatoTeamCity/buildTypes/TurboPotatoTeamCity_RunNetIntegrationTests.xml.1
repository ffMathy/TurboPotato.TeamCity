<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="bfb0f32b-164c-442f-bad3-5cd09194c9a9" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2018.1/project-config.xsd">
  <name>Run .NET Integration Tests</name>
  <description />
  <settings>
    <parameters />
    <build-runners>
      <runner id="RUNNER_2" name="" type="dotnet.cli">
        <parameters>
          <param name="command" value="vstest" />
          <param name="dotNetCoverage.dotCover.home.path" value="%teamcity.tool.JetBrains.dotCover.CommandLineTools.DEFAULT%" />
          <param name="paths" value="*.Tests.dll" />
          <param name="teamcity.step.mode" value="default" />
          <param name="test.filter" value="filter" />
          <param name="test.testCaseFilter" value="TestCategory=Unit" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="MyProject_TurboPotatoTeamCity" />
    </vcs-settings>
    <requirements />
    <build-triggers />
    <artifact-dependencies>
      <dependency id="ARTIFACT_DEPENDENCY_1" sourceBuildTypeId="MyProject_CompileNet" cleanDestination="true">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="binaries.zip!**=&gt;" />
      </dependency>
    </artifact-dependencies>
    <dependencies>
      <depend-on sourceBuildTypeId="MyProject_CompileNet">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

